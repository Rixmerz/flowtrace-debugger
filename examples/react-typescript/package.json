{
  "name": "flowtrace-react-typescript-example",
  "version": "1.0.0",
  "description": "FlowTrace example with React + TypeScript - Hooks, Context API, and TypeScript decorators",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:trace": "FLOWTRACE_PACKAGE_PREFIX=src FLOWTRACE_LOG_FILE=flowtrace.jsonl FLOWTRACE_STDOUT=false node --require ../../flowtrace-agent-js/src/loader.js ./node_modules/.bin/vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "trace": "node run-with-flowtrace.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
